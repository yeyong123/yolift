<% provide(:title, @ship.title) %>
<div class="edge-position">
	<%= render 'home/nav_list' %>
	<div class="hr-postion">
	</div>
	<div class="edge-from">
		<ul>
			<% Ship.all.each do |ship| %>
				<li><%= link_to ship.title, ship_path(ship.id) %></li>
			<% end %>
		</ul>
	</div>
</div>
<div class="product-index">
	<div class="about-title">
		<h4><%= @ship.title %></h4>
		<div class="breadcrumb">
			<%= render_navigation(renderer: :breadcrumbs, join_with: '&nbsp / &nbsp') %>
		</div>
	</div>
	<div class="about-content">
		<%= raw @ship.content %>
	</div>
</div>
