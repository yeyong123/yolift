<div class="product-show">
<span>产品名称: <b><%= @product.title %></b></span><br/>
 <span>货号：</span>
<% @product.productnumbers.each do |ph| %>
	<span><%= ph.number %>,</span>
<% end %>
</div>
<div class="flexslider-show">
<ul class="slides">
<% @product.photos.each do |pho| %>
	<li data-thumb="<%= pho.image_url(:navshow)%>">
	<%= image_tag(pho.image_url(:thumb)) %>
	</li>
<% end %>
</ul>
</div>
<div class="mobile-product-show">
<%=  sanitize @product.techparams %>
</div>


