<%= render 'home/nav_list' %>
<div class="product-index">
	<h4>申请产品目录</h4>
	<hr/>
		<%= form_for @catelog do |f| %>
			<%= render 'home/error_message', object: f.object %> 
			<div class="catelog-image">
				<%= image_tag(@catelog.image_url) if @catelog.image? %>
			</div>
			<%= render partial: 'catelogs/form', locals: {f: f} %>
		<% end %>
</div>
