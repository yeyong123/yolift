<div class="product-index">
	<div class="about-title">
		<h4>客户详细信息</h4>
		<div class="breadcrumb">
	</div>
</div>
<table class="table">
	<tr>
		<td><%= @catelog.created_at.strftime("%Y-%m-%d:%H:%M")%></td>
		<td><%= render partial: 'catelogs/deal', locals: {cate: @catelog  } %></td>
	</tr>
	<tr>
		<td>客户公司:</td>
		<td><%= @catelog.company %></td>
		<td>客户姓名：</td>
		<td><%= @catelog.name %></td>
		<td>客户手机：</td>
		<td><%= @catelog.mobel %></td>
	</tr>
	<tr>
		<td>地址：</td>
		<td colspan="5"><%= @catelog.province.name %>
			<%=@catelog.city.name %>
			<%= @catelog.district.name %>
			<%= @catelog.address %>
		</td>
	</tr>
	<tr>
		<td>申请样册数量：</td>
		<td><%= @catelog.number %></td>
		<td>电话：</td>
		<td><%= @catelog.phone %></td>
		<td>邮编:</td>
		<td><%= @catelog.zip %></td>
	</tr>
	<tr>
		<td>感兴趣的产品：</td>
		<td><%= @catelog.interest %></td>
		<td>邮箱：</td>
		<td><%= @catelog.email %></td>
	</tr>
</table>
</div>
