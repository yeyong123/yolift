<%= render 'home/nav_list' %>
<div class="product-index">
	<%= form_for @about do |f| %>
		<div class="field">
			<%= f.label :image, "导航图展示"%>
			<%= image_tag(@about.image_url(:small)) if @about.image? %>
	<%= f.file_field :image %>
</div>
<%= render partial: "lists/form", locals: {f: f} %>
<div class="about-action">
	<%= f.submit "保存", class: "btn btn-success" %>
	<%= link_to "返回", abouts_path %>
</div>
<% end %>
</div>
