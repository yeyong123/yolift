<% provide(:title, @list.title) %>
<%= render 'home/nav_list' %>
<div class="product-index">
	<div class="about-title">
		<h4><%= @list.title %></h4>
		<div class="breadcrumb">
			<%= render_navigation(renderer: :breadcrumbs, join_with: '&nbsp / &nbsp') %>
		</div>
	</div>
	<div class="about-content">
		<table class="table">
			<% @list.guides.each do |guide| %>
				<tr>
					<td><%= link_to guide.title, guide_path(guide) %></td>
				</tr>
			<% end %>
			</table>
		</div>

</div>
