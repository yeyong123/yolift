<%= render 'products/nav_list' %>
<div id="customer-index-img">
	<div class="get-image-position">
		<div id="myCarousel" class="carousel slide">
			<ol class="carousel-indicators">
				<% Bananer.all.each_with_index do |bananer,index| %>
					<li data-target="#myCarousel" data-slide-to='<%= index %>'></li>
				<% end %>
			</ol>
			<div class="carousel-inner">
				<% Bananer.all.each do |bananer| %>
					<div class="item">
						<%= link_to image_tag(bananer.image_url), bananer.button %>
						</div>
				<% end %>
			</div>
		</div>
	</div>
</div>
	<div class="position-none">
	</div>
	<%= render 'home/toggle_tab' %>

