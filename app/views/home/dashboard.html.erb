<div id="myTabContent" class="tab-content">
	<div class="tab-pane fade active in" id="one">
		<table class="table">
			<% @products.each do |product| %>
				<tr>
					<td><%= link_to product.title, product_path(product) %></td>
					<% product.productnumbers.limit(1).each do |pr| %>
						<td><%= pr.number %></td>
					<% end %>
					<td><%= File.basename(product.printpdf_url) if product.printpdf?%></td>
					<td><%= image_tag(product.photos.first.image_url(:small)) %></td>
					<td>
						<%= form_for product do |f| %>
							<% if !product.excellent?%>
							<%= check_box_tag('product[excellent]', 1)%>
							<%= f.submit "推荐展示", class: "btn btn-mini btn-success" %>
						<% else %>
							<%= check_box_tag('product[excellent]',0) %>
							<%= f.submit "取消推荐", class: "btn btn-mini btn-danger" %>
						<% end %>
						<% end %>
						</td>
					<td><%=time_ago_in_words(product.created_at) %>前</td>
					<td><%= link_to "编辑", edit_product_path(product) %></td>
					<td><%= link_to "删除", product, method: :delete, confirm: "确认删除" %></td>
				</tr>
			<% end %>
		</table>
		<%= will_paginate @products %>
		<%= link_to "增加产品", new_product_path, class: "btn btn-primary" %>
	</div>
	<div class="tab-pane fade" id="two">
		<table class="table">
			<% @categories.each do |cate| %>
				<tr>
					<td><%= link_to cate.name, category_path(cate) %></td>
					<td><%= link_to "编辑", edit_category_path(cate) %></td>
					<td><%= link_to "删除", cate, method: :delete, confirm: "确认删除" %></td>
				</tr>	
			<% end %>
		</table>
	</div>
	<div class="tab-pane fade" id="three">
