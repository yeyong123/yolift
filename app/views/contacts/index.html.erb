<% provide(:title, "联系我们") %>
<%= render 'home/nav_list' %>
<div class="product-index">
	<table class="table">
		<% @contacts.each do |contact| %>
			<tr>
				<td><%= link_to contact.title, contact_path(contact) %></td>
				<td><%= contact.created_at.strftime("%Y-%m-%d") %>创建</td>
			</tr>
		<% end %>
	</table>
</div>
