<%= render 'home/nav_list' %>
<div class="product-index">
	<%= form_for @item do |f| %>
		<%= render partial: "lists/form", locals: {f: f} %>
	<div class="about-action">
		<%= f.submit "保存", class: "btn btn-success" %>
		<%= link_to "返回", items_path, class: "btn btn-primary"%>
	</div>
<% end %>
</div>
